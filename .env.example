# Секретный ключ Django (замените на сгенерированный перед запуском)
DJANGO_SECRET_KEY=your-secret-key-here

# Режим отладки: 1 = включен, 0 = выключен
DJANGO_DEBUG=1

# Разрешённые хосты (через запятую, без пробелов)
# Например: 127.0.0.1,localhost,mydomain.com
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost